<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="472.0" prefWidth="284.0" spacing="20.0"
   xmlns="http://javafx.com/javafx/21"
   xmlns:fx="http://javafx.com/fxml/1" fx:controller="hi.vidmot.ListiController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <ImageView fitHeight="63.0" fitWidth="241.0" pickOnBounds="true" preserveRatio="true" />
      <ListView fx:id="fxListView" onMouseClicked="#onValidLag" prefHeight="237.0" prefWidth="244.0" />
      <ImageView fitHeight="53.0" fitWidth="242.0" pickOnBounds="true" preserveRatio="true" />
      <ProgressBar prefHeight="20.0" prefWidth="241.0" progress="0.0" />
      <ButtonBar prefHeight="40.0" prefWidth="337.0">
         <buttons>
            <Button mnemonicParsing="false" prefHeight="37.0" text="Heim" />
            <Button fx:id="fxPlayPause" mnemonicParsing="false">
               <graphic>
                  <ImageView fitHeight="21.0" fitWidth="57.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Button>
         </buttons>
      </ButtonBar>
   </children>
</VBox>
